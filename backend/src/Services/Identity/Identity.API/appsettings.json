{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=__DB_HOST__;Port=5432;Database=chat_db;Username=__DB_USER__;Password=__DB_PASSWORD__"
  },
  "JwtSettings": {
    "SecretKey": "__JWT_SECRET_KEY__",
    "Issuer": "ChatApp",
    "Audience": "ChatAppUsers",
    "ExpiryMinutes": "60"
  }
}