{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=__DB_HOST__;Port=5432;Database=user_db;Username=__DB_USER__;Password=__DB_PASSWORD__"
  },
  "EventBusSettings": {
    "HostAddress": "amqp://__RABBITMQ_USER__:__RABBITMQ_PASS__@__RABBITMQ_HOST__:5672"
  },
  "Jwt": {
    "Key": "__JWT_SECRET_KEY__",
    "Issuer": "ChatApp",
    "Audience": "ChatAppUsers"
  }
}