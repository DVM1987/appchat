{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "Redis": "__REDIS_HOST__:6379"
    },
    "JwtSettings": {
        "SecretKey": "__JWT_SECRET_KEY__",
        "Issuer": "ChatApp",
        "Audience": "ChatAppUsers"
    }
}